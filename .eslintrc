{
    "extends": [
        "@joinbox/joinbox",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-use-before-define": 0,
        //"no-console": 0,
        "no-mixed-operators": 0
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "legacyDecorators": true
        }
    }
}